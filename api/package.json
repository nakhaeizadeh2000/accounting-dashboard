  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --host 0.0.0.0 --port 4000",
    "start:debug": "nest start --debug --watch --host 0.0.0.0 --port 4000",
    "start:prod": "node dist/src/main",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run build && npm run typeorm -- migration:create",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d ./config/database/postgres/typeorm.ts",
    "migration:revert": "npm run build && npm run typeorm -- migration:revert -d ./config/database/postgres/typeorm.ts",
    "seed:run": "npm run build && ts-node -r tsconfig-paths/register src/db/seeders/seed.micro-app.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:e2e": "ts-node test/test-environment.ts && jest --config ./jest.config.js",
    "test:watch": "ts-node test/test-environment.ts && jest --config ./jest.config.js --watch",
    "open:report": "open ./test-report/report.html || xdg-open ./test-report/report.html || start ./test-report/report.html"
  },
